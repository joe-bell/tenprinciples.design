{
  "name": "tenprinciples.design",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "pnpm build:css && pnpm astro check && astro build",
    "build:css": "pnpm tailwind --minify",
    "dev": "pnpm build:css && pnpm run '/^dev:.*/'",
    "dev:astro": "astro dev",
    "dev:css": "pnpm tailwind --watch",
    "prepare": "pnpm run '/^prepare:.*/'",
    "prepare:hooks": "git config core.hooksPath .github/hooks",
    "prettier": "prettier --config .prettierrc.json --ignore-unknown --no-error-on-unmatched-pattern",
    "preview": "astro preview",
    "start": "astro dev",
    "tailwind": "pnpx @tailwindcss/cli -i ./src/styles/input.css -o ./src/styles/output.css"
  },
  "dependencies": {
    "@astrojs/check": "0.9.5",
    "astro": "^5.16.0"
  },
  "devDependencies": {
    "@astrojs/cloudflare": "12.6.11",
    "@astrojs/ts-plugin": "1.10.5",
    "@tailwindcss/cli": "4.1.17",
    "@types/node": "22.14.1",
    "lint-staged": "16.2.6",
    "prettier": "3.5.3",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-packagejson": "2.5.19",
    "prettier-plugin-tailwindcss": "0.7.1",
    "tailwindcss": "4.1.17",
    "typescript": "5.0.4",
    "wrangler": "4.48.0"
  },
  "packageManager": "pnpm@10.23.0",
  "engines": {
    "node": "22"
  }
}
